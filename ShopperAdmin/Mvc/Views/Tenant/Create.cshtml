@{
    Layout = "_Layout";
}

@section css{
    <style>
        fieldset
                	{
                		border: 1px solid #ddd !important;
                		margin: 0;
                		xmin-width: 0;
                		padding: 10px;
                		position: relative;
                		border-radius:4px;
                		background-color:#f5f5f5;
                		padding-left:10px!important;
                	}

                		legend
                		{
                			font-size:14px;
                			font-weight:bold;
                			margin-bottom: 0px;
                			background-color : darkgray;
                			width: 35%;
                			border: 2px solid darkgray;
                			border-radius: 4px;
                			padding: 5px 5px 5px 10px;
                			/*background-color: #ffffff;*/
                		}
    </style>
}

<div class="card card-default">
    <div class="card-body">
        <th:form class="" asp-action="Store" method="post">
            <div class="modal-body">
                <th:partial name="_TenantForm" model="new CreateTenantModel()" view-data="ViewData"/>
            </div>
        </th:form>
    </div>
</div>

@section scripts
{
	<th:partial name="Partials/_DtJs"></th:partial>
	<th:partial name="_ValidationScriptsPartial"></th:partial>
	<th:partial name="Partials/_ToastScript"></th:partial>
}
